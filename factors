#!/usr/bin/env python3
import sys
import math

def find_factors(n):
    """Find two factors of n such that n = p * q."""
    if n <= 1:
        return None
    # Check divisibility starting from the smallest prime number
    limit = int(math.sqrt(n)) + 1
    for p in range(2, limit):
        if n % p == 0:
            q = n // p
            if p * q == n:
                return (p, q)
    return None

def process_file(filename):
    """Read numbers from file, factorize each, and print results."""
    with open(filename, 'r') as file:
        for line in file:
            number = int(line.strip())
            factors = find_factors(number)
            if factors:
                p, q = factors
                print(f"{number}={p}*{q}")

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)

    filename = sys.argv[1]
    process_file(filename)
